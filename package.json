{
  "name": "lantern",
  "version": "0.0.1",
  "description": "Lantern - a data analytics platfor for the FT",
  "main": "src/server/server.js",
  "scripts": {
    "test": "env NODE_ENV=test mocha --compilers js:babel/register --recursive test/**/*.spec.js",
    "test-debug": "env NODE_ENV=test mocha --debug-brk --compilers js:babel/register --recursive test/**/*.spec.js",
    "clean": "rm -rf lib",
    "watch-js": "babel src -d lib --experimental -w",
    "dev-server": "env $(cat .env) node lib/server/webpack",
    "server": "nodemon lib/server/server",
    "postinstall": "npm run build && env $(cat .env) webpack --config webpack.config.prod.js",
    "start": "npm run watch-js & npm run dev-server & env $(cat .env) npm run server",
    "build": "npm run clean && babel src -d lib --experimental"
  },
  "author": "Dario Villanueva",
  "license": "MIT",
  "jshintConfig": {
    "esnext": true
  },
  "engines": {
    "node": "0.12.7"
  },
  "dependencies": {
    "alt": "^0.17.1",
    "assert": "^1.3.0",
    "autoprefixer-loader": "^3.1.0",
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "body-parser": "^1.13.3",
    "bootstrap-daterangepicker": "^2.0.11",
    "bunyan": "^1.5.1",
    "c3": "^0.4.10",
    "compression": "^1.5.2",
    "connect-redis": "^2.5.0",
    "cookie-parser": "^1.3.5",
    "css-loader": "^0.18.0",
    "datamaps": "^0.4.0",
    "elasticsearch": "^8.0.1",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "express-session": "^1.11.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "iso": "^4.2.0",
    "mocha": "^2.2.5",
    "moment": "^2.10.6",
    "moment-range": "^2.0.3",
    "node-sass": "^3.3.3",
    "nodemon": "^1.4.1",
    "object-assign": "^4.0.1",
    "passport": "^0.3.0",
    "passport-google-oauth2": "^0.1.6",
    "raven": "^0.8.1",
    "raven-js": "^1.1.20",
    "react": "^0.13.3",
    "react-bootstrap": "^0.25.0",
    "react-daterange-picker": "^0.12.0",
    "react-document-title": "^2.0.0",
    "react-google-analytics": "^0.2.0",
    "react-hot-loader": "^1.2.8",
    "react-router": "^0.13.3",
    "redis": "^1.0.0",
    "sass-loader": "^2.0.1",
    "style-loader": "^0.12.4",
    "superagent": "^1.3.0",
    "underscore": "^1.8.3",
    "uuid": "^2.0.1",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "devDependencies": {
    "chai": "^3.2.0"
  }
}
