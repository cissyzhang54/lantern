{
  "name": "lantern",
  "version": "0.0.1",
  "description": "Lantern - a data analytics platfor for the FT",
  "main": "src/server/server.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register --recursive test/**/*.spec.js",
    "test-debug": "mocha --debug-brk --compilers js:babel/register --recursive test/**/*.spec.js",
    "clean": "rm -rf lib",
    "watch-js": "./node_modules/.bin/babel src -d lib --experimental -w",
    "dev-server": "node lib/server/webpack",
    "server": "nodemon lib/server/server",
    "postinstall": "npm run build",
    "start": "npm run watch-js & npm run dev-server & npm run server",
    "build": "npm run clean && ./node_modules/.bin/babel src -d lib --experimental"
  },
  "author": "Dario Villanueva",
  "license": "MIT",
  "jshintConfig": {
    "esnext": true
  },
  "dependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "body-parser": "^1.13.3",
    "express": "^4.13.3",
    "express-handlebars": "^2.0.1",
    "mocha": "^2.2.5",
    "nodemon": "^1.4.1",
    "react": "^0.13.3",
    "react-hot-loader": "^1.2.8",
    "react-router": "^0.13.3",
    "react-tools": "^0.13.3",
    "testdom": "^1.1.0",
    "uuid": "^2.0.1",
    "webpack": "^1.11.0",
    "webpack-dev-server": "^1.10.1"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "jsdom": "^6.1.0"
  }
}
